{
    "$schema": "https://swc.rs/schema.json",
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "decorators": true,
            "dynamicImport": true
        },
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        },
        "target": "es2021",
        "keepClassNames": true,
        "baseUrl": ".",
        "paths": {
            "#/*": ["./src/*"]
        }
    },
    "module": {
        "type": "commonjs"
    }
}
